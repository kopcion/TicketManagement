<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Menu">
   <children>
      <TabPane prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab closable="false" text="Manage tickets">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ListView fx:id="searchResults" layoutX="14.0" layoutY="39.0" onMouseClicked="#handleSearchField" prefHeight="330.0" prefWidth="248.0" />
                        <TextField fx:id="searchField" layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="181.0" />
                        <Button fx:id="confirmButton" layoutX="210.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#handleConfirmButton" text="Search" />
                     </children>
              </AnchorPane>
            </content>
          </Tab>
          <Tab closable="false" text="Upcoming events">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ListView fx:id="upcoming" layoutX="14.0" layoutY="14.0" prefHeight="336.0" prefWidth="260.0" />
                     </children></AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
